	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<Button name="DMResizeButton" virtual="true">
		<Size x="24" y="24"/>
		<NormalTexture file="Interface\Buttons\UI-Panel-SmallerButton-Up">
			<TexCoords left="1" right="0" top="0" bottom="1"/>
		</NormalTexture>
		<PushedTexture file="Interface\Buttons\UI-Panel-SmallerButton-Down">
			<TexCoords left="1" right="0" top="0" bottom="1"/>
		</PushedTexture>
		<HighlightTexture file="Interface\Buttons\UI-Panel-MinimizeButton-Highlight" alphaMode="ADD"/>
	</Button>

	<Frame name="DMinstrelUI" parent="UIParent" movable="true" hidden="false" clampedToScreen="true">
		<Size x="320" y="380"/>
		<Anchors>
			<Anchor point="CENTER" x="0" y="-50"/>
		</Anchors>
		<Scripts>
			<OnLoad>
				tinsert(UISpecialFrames, self:GetName());
				self:Show();
				self:RegisterForDrag("LeftButton");
			</OnLoad>
			<OnDragStart>self:StartMoving();</OnDragStart>
			<OnDragStop>self:StopMovingOrSizing();</OnDragStop>
		</Scripts>
		<Backdrop edgeFile="Interface\GLUES\COMMON\TextPanel-Border" tile="true">
			<EdgeSize>
				<AbsValue val="16"/>
			</EdgeSize>
			<BackgroundInsets>
				<AbsInset left="3" right="3" top="3" bottom="3"/>
			</BackgroundInsets>
		</Backdrop>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture parentKey="bg" atlas="auctionhouse-background-index">
					<Anchors>
						<Anchor point="TOPLEFT" realtiveTo="$parent" relativePoint="TOPLEFT" x="3" y="0"/>
						<Anchor point="BOTTOMRIGHT" realtiveTo="$parent" relativePoint="BOTTOMRIGHT" x="0" y="3"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<Frames>
			<!-- RESIZING BUTTON -->
			<Button parentKey="Resize" inherits="DMResizeButton">
				<Size x="24" y="24"/>
				<KeyValues>
					<KeyValue key="minWidth" value="320" type="number"/>
					<KeyValue key="minHeight" value="380" type="number"/>
				</KeyValues>
				<Anchors>
					<Anchor point="BOTTOMRIGHT" x="3" y="-3"/>
				</Anchors>
				<Scripts>
					<OnLoad>
						DMinstrel:InitResize(self);
					</OnLoad>
				</Scripts>
			</Button>
			<Button parentKey="DMClose">
				<Size x="32" y="32"/>
				<Anchors>
					<Anchor relativeTo="DMinstrelUI" relativePoint="TOP" x="-14" y="13"/>
				</Anchors>
				<NormalTexture file="Interface\AddOns\DMinstrel\Resources\IMG\darkmoon"/>
				<Scripts>
					<OnEnter>
						self:SetNormalTexture("Interface/AddOns/DMinstrel/Resources/IMG/darkmoon-closed");
		                GameTooltip:SetOwner(self, "ANCHOR_CURSOR");
		                GameTooltip:AddLine("|cFFFFFFFFDarkmoon Minstrel Addon|r");

                        GameTooltip:AddDoubleLine("Created by:", "TestUnit", 1, 1, 1, 1, 0.96, 0.41);
                        GameTooltip:AddLine(" ");
                        GameTooltip:AddDoubleLine("Версия:", "0.92 BFA", 1, 1, 1, 1, 0.96, 0.41);
                        GameTooltip:AddDoubleLine("Ролевой сервер Darkmoon:", "rp-wow.ru", 1, 1, 1, 1, 0.96, 0.41);
		                GameTooltip:Show();
					</OnEnter>
					<OnLeave>
						self:SetNormalTexture("Interface/AddOns/DMinstrel/Resources/IMG/darkmoon");
						GameTooltip:Hide();
					</OnLeave>
					<OnClick>
						DMinstrelUI:Hide();
					</OnClick>
				</Scripts>
			</Button>
